@model MqttServerTest.DeviceModel

<div class="container">
    <form asp-action="Add">
        <div><label>  </label></div>
        <input type="hidden" asp-for="User" />
        <input type="hidden" asp-for="Message" />
        <div class="form-group row">
            <label asp-for="Name" class="col-sm-2 col-form-label">设备名称</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" asp-for="Name">
            </div>
        </div>
        <div class="form-group row">
            <label asp-for="DeviceKey" class="col-sm-2 col-form-label">设备秘钥</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" asp-for="DeviceKey">
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <button type="submit" class="btn bin-primary">提交</button>
            </div>
        </div>
    </form>
</div>
